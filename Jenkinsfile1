pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                checkout scm
            }
        }
        stage('build'){
            steps {
                sh 'mvn install'
            }
                      }
        stage('Dockerfile true') {
            steps {
                sh 'docker build -t="jaisriram111/mypip-img" .'
            }
        }

        stage('creat-docenv.sh true') {
            steps{
		sh 'creat-docenv.sh'
	    }
	}
  }
 }
