pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                checkout scm
            }
        }
        stage('build'){
            steps {
                sh 'mvn install'
            }
                      }
            }  
    agent { dockerfile true }
    stages {
        stage('build') {
            steps {
                sh 'docker build -t="jaisriram111/mypip-img" .'
                sh 'creat-docenv.sh'
            }
        }
    }
        
   }

